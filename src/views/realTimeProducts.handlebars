<div id="productos">

{{#if carrito.length}}
{{#each carrito}}
    <h2>Carrito {{id}}:</h2>
    {{#each products}}
    <p>Id del producto {{id}} / Cantidad {{quantity}} </p>
    {{/each}}
    <br>
{{/each}}
{{else}}
    <p>No existen carritos</p>
{{/if}}

</div>

<script src="/socket.io/socket.io.js"></script>
<script src="js/index.js"></script>